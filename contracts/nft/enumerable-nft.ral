import "std/nft_interface"

// Only 1 token is supported
Contract EnumerableNFT(
    collection: NFTPreDesignedCollection,
    nftIndex: U256
) implements INFT {
    pub fn getTokenUri() -> ByteVec {
        return collection.getTokenUri(nftIndex)
    }

    pub fn getCollectionId() -> ByteVec {
        return contractId!(collection)
    }
}
