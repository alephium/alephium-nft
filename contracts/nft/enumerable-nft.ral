import "std/nft_interface"

// Only 1 token is supported
Contract EnumerableNFT(
    collectionId: ByteVec,
    nftIndex: U256
) implements INFT {
    // name; description; image
    pub fn getTokenUri() -> ByteVec {
        checkCaller!(true, 0)
        return NFTPreDesignedCollection(collectionId).getTokenUri(nftIndex)
    }

    pub fn getCollectionId() -> ByteVec {
        return collectionId
    }
}
