//import "std/nft_interface"

Contract NFT(
  tokenUri: ByteVec,
  collectionId: ByteVec,
  nftIndex: U256
) implements INFT {
  pub fn getTokenUri() -> ByteVec {
    return tokenUri
  }

  pub fn getNFTIndex() -> U256 {
    return nftIndex
  }

  pub fn getCollectionIndex() -> (ByteVec, U256) {
    return collectionId, nftIndex
  }
}