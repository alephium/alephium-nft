TxScript ListNFT(
    tokenId: ByteVec,
    price: U256,
    nftMarketplace: NFTMarketPlace
) {
    let listingFee = nftMarketplace.getListingFee()
    let minimalAlphInContract = 1 alph
    let approveAmount = listingFee + minimalAlphInContract

    nftMarketplace.listNFT{callerAddress!() -> ALPH: approveAmount, tokenId: 1}(tokenId, price)
}
